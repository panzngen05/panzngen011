                                    <div class="flex items-center">
                                        <div class="checkbox">
                                            <input type="checkbox"  <%- config.protection.antilinks.deleteMessages == 'true' && "checked" %> id="cbx-5" class="inp-cbx" name="antilinks.deleteMessages"/>
