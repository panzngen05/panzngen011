                                    <input type="radio" class="radio-button__input" id="radio1" name="radio-group" value="antilinks.actions.timeout" <%-  config.protection.antilinks.actions.timeout == "true" && "checked"%>>
                                    <label class="radio-button__label" for="radio1">
                                      <span class="radio-button__custom"></span>
                                      Timeout
                                    </label>
                                  </div>
                                  <div class="radio-button">
                                    <input type="radio" class="radio-button__input" id="radio2" name="radio-group"  value="antilinks.actions.kick" <%-  config.protection.antilinks.actions.kick == "true" && "checked"%>>
                                    <label class="radio-button__label" for="radio2">
