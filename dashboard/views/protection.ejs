<div class="page">
    <div class="container mx-auto">
        <form id="config.protection-form" action="/protection" method="post" class="space-y-8 divide-y divide-gray-200">
            <!-- General Bot Settings -->
            <div class="space-y-8 divide-y divide-gray-200">
                <div class="autoResponderEntry mb-4 p-4 border border-gray-200 rounded-[15px]">
                    <h2 class="text-lg leading-6 font-medium text-white">General Bot Settings</h2>
                    <div class="mt-6 grid grid-cols-1 gap-y-6 gap-x-4 sm:grid-cols-6">
                        <div class="sm:col-span-3">
                            <label for="allowedGuildId" class="block text-sm font-medium text-white mb-2">Allowed Guild
                                ID:</label>
                            <input type="text" required id="allowedGuildId" name="allowedGuildId"
                                value="<%= config.protection.allowedGuildId %>" class="input2 w-[100%]">
                        </div>

                        <div class="sm:col-span-3">
                            <label for="logChannelId" class="block text-sm font-medium text-white mb-2">Log Channel
                                ID:</label>
                            <input type="text" required id="logChannelId" name="logChannelId"
                                value="<%= config.protection.logChannelId %>" class="input2 w-[100%]">
                        </div>

                        <div class="sm:col-span-6">
                            <label for="whitelistRoles" class="block text-sm font-medium text-white mb-2">Whitelist
                                Roles:</label>
                            <div id="whitelistRoles" class="whitelistgrid">
                                <% config.protection.whitelistRoles.forEach(function(role, index) { %>
                                    <input type="text" required name="whitelistRoles[<%= index %>]" value="<%= role %>"
                                        class="input w-[100%]">
                                    <% }) %>
                            </div>
                            <div class="rolebutton flex">
                                <button type="button" onclick="addRoleInput()"
                                    class="ui-btn bg-[#010314] mt-5"><span>Add Role</span></button>
                                <button type="button" onclick="removeLastRole()"
                                    class="ui-btn bg-[#010314] mt-5"><span>Delete Role</span></button>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Anti-spam Settings -->
                <div class="pt-8">
                    <div class="autoResponderEntry mb-4 p-4 border border-gray-200 rounded-[15px]">
                        <h2 class="text-lg leading-6 font-medium text-white">Anti-spam Settings</h2>
                        <div class="mt-6">
                            <div class="flex items-center">
                                <div class="checkbox">
                                    <input type="checkbox"  <%- config.protection.antispam.active == 'true' && "checked" %> id="cbx-1" class="inp-cbx" name="antispam.active"/>
                                    <label for="cbx-1" class="cbx">
                                        <span>
                                            <svg viewBox="0 0 12 10" height="10px" width="12px">
                                                <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                            </svg>
                                        </span>
                                    </label>
                                </div>
                                <span class="ml-2 text-sm text-white">Active</span>
                            </div>
                        </div>
                        <div class="mt-4 grid grid-cols-1 gap-y-6 gap-x-4 sm:grid-cols-6">
                            <div class="sm:col-span-3">
                                <label for="antispam-messageCount"
                                    class="block text-sm font-medium text-white mb-2">Message Count:</label>
                                <input type="number" required name="antispam.messageCount" id="antispam-messageCount"
                                    class="input2" value="<%= config.protection.antispam.messageCount %>"
                                    placeholder="5">
                            </div>

                            <div class="sm:col-span-3">
                                <label for="antispam-time" class="block text-sm font-medium text-white mb-2">Time
                                    (ms):</label>
                                <input type="number" required name="antispam.time" id="antispam-time" class="input2"
                                    value="<%= config.protection.antispam.time %>" placeholder="10000">
                            </div>
                        </div>
                        <h2 class="text-lg leading-6 font-medium text-white mt-4">Actions</h2>
                        <div class="mt-4 grid grid-cols-1 gap-y-6 gap-x-4 sm:grid-cols-6">
                            <div class="flex items-center">
                                <div class="checkbox">
                                    <input type="checkbox"  <%- config.protection.antispam.deleteMessages == 'true' && "checked" %> id="cbx-2" class="inp-cbx" name="antispam.deleteMessages"/>
                                    <label for="cbx-2" class="cbx">
                                        <span>
                                            <svg viewBox="0 0 12 10" height="10px" width="12px">
                                                <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                            </svg>
                                        </span>
                                    </label>
                                </div>
                                <span class="ml-2 text-sm text-white">Delete Messages</span>
                            </div>
                        </div>            
                        <div class="mt-4 grid grid-cols-1 gap-y-6 gap-x-4 sm:grid-cols-6">
                            <div class="flex items-center">
                                <div class="checkbox">
                                    <input type="checkbox"  <%- config.protection.antispam.timeout == 'true' && "checked" %> id="cbx-3" class="inp-cbx" name="antispam.timeout"/>
                                    <label for="cbx-3" class="cbx">
                                        <span>
                                            <svg viewBox="0 0 12 10" height="10px" width="12px">
                                                <polyline points="1.5 6 4.5 9 10.5 1"></polyline>
                                            </svg>
                                        </span>
                                    </label>
                                </div>
                                <span class="ml-2 text-sm text-white">Timeout</span>
                            </div>
                        </div>  
                        <div class="mt-4 grid grid-cols-1 gap-y-6 gap-x-4 sm:grid-cols-6">
                            <div class="sm:col-span-6">
                                <label for="antispam-time" class="block text-sm font-medium text-white mb-2">Timeout Duration
                                    (ms):</label>
                                <input type="number" required name="antispam.timeoutDuration" id="antispam-time" class="input2"
                                    value="<%= config.protection.antispam.timeoutDuration %>" placeholder="10000">
                            </div>
                        </div>            
                    </div>
                </div>
                <!-- Anti-links Settings -->
                <div class="pt-8">
                    <div class="autoResponderEntry mb-4 p-4 border border-gray-200 rounded-[15px]">
                        <h2 class="text-lg leading-6 font-medium text-white">Anti-links Settings</h2>
